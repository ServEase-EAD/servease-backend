[run]
source = accounts
omit = 
    */migrations/*
    */tests/*
    */__pycache__/*
    */venv/*
    */virtualenv/*
    */.venv/*
    manage.py
    */asgi.py
    */wsgi.py
    */settings.py
    */apps.py
    */admin.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

precision = 2
show_missing = True
skip_covered = False

[html]
directory = htmlcov
