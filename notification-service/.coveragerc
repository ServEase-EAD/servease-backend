[run]
source = app_notifications
omit = 
    */tests/*
    */migrations/*
    */venv/*
    */virtualenv/*
    */__pycache__/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = htmlcov
